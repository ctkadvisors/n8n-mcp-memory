{
  "name": "n8n-mcp",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "start:v2": "node dist/indexV2.js",
    "start:server": "node dist/server.js",
    "start:enhanced": "node dist/server-enhanced.js",
    "start:advanced": "node dist/advanced.js",
    "update-docs": "node dist/scripts/updateDocumentation.js",
    "client": "node dist/client.js",
    "client:v2": "node dist/clientV2.js",
    "n8n-client": "node dist/n8n-client.js",
    "n8n-client:v2": "node dist/n8n-client-v2.js",
    "task-client": "node dist/task-client.js",
    "workflow-client": "node dist/workflow-client.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:http": "node tests/http-streaming/fetch-test.js",
    "test:execution": "node tests/http-streaming/execution-test.js",
    "test:credential": "node tests/http-streaming/credential-test.js",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write \"src/**/*.{js,ts}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts}\"",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "CTK Advisors",
  "license": "MIT",
  "description": "MCP HTTP Streaming Server with n8n Integration",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.10.2",
    "@types/express": "^5.0.1",
    "axios": "^1.8.4",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "form-data": "^4.0.2",
    "node-fetch": "^3.3.2",
    "openai": "^4.36.0",
    "openapi-typescript-codegen": "^0.29.0",
    "pg": "^8.11.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/jest": "^29.5.14",
    "@types/pg": "^8.11.2",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nock": "^14.0.4",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "typedoc": "^0.25.12"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
